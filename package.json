{
  "name": "CleanScreen",
  "version": "0.1.0",
  "description": "An app that turns the screen black to help clean the screen. Built via Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": "https://github.com/padi/CleanYoScreen",
  "keywords": [
    "Electron",
    "Utilities",
    "clean",
    "screen"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.70",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@electron-forge/publisher-github": "^6.0.0-beta.68",
    "electron": "^21.1.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icons/CleanScreen"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "padi",
              "name": "CleanScreen"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
